<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">

	<relations>
		<relation code="Customer2Organisation" autocreate="true" localized="false">
			<deployment table="CustomersOrganisationsRel" typecode="1337" />
			<sourceElement type="Customer" qualifier="customers" cardinality="many"></sourceElement>
			<targetElement type="Organisation" qualifier="organisations" cardinality="many"></targetElement>
		</relation>
	</relations>		
			
	<itemtypes>
		<itemtype code="Customer" autocreate="false" generate="false">
			<attributes>
				<attribute qualifier="status" type="java.lang.Boolean" generate="true">					
